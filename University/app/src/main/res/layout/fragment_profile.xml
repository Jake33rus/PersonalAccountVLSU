                app:layout_constraintEnd_toStartOf="@+id/text_view_email"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/text_view_email"
                app:srcCompat="@drawable/ic_at" />

            <TextView
