                android:text="Студент ИТу-117"
                android:textSize="20sp"
                android:textStyle="italic"
                app:layout_constraintStart_toEndOf="@+id/imageViewPhoto"
                app:layout_constraintTop_toBottomOf="@+id/text_view_fio" />

            <TextView
