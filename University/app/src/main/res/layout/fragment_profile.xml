            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/imageView13"
                android:layout_width="0dp"
                android:layout_height="0dp"
                android:layout_marginStart="16dp"
                android:layout_marginEnd="16dp"
                app:layout_constraintBottom_toBottomOf="@+id/text_view_finansValue"
                app:layout_constraintEnd_toStartOf="@+id/text_view_finansValue"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="@+id/text_view_finans"
                app:srcCompat="@drawable/ic_cash_multiple" />

            <androidx.appcompat.widget.AppCompatImageView
                android:id="@+id/imageView14"
                android:layout_width="0dp"
